{
    "name": "stratedge/inspection",
    "description": "A command-line-utility for generating a data file from docblocks with @api-* tags",
    "type": "library",
    "keywords": [
        "documentation",
        "api",
        "docblock",
        "apidoc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jarret Byrne",
            "email": "jarret@solvomedia.com",
            "homepage": "http://jarretbyrne.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/stratedge/inspection/issues",
        "source": "https://github.com/stratedge/inspection"
    },
    "require": {
        "symfony/finder": "^2.8",
        "symfony/console": "^2.8",
        "phpdocumentor/reflection-docblock": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Stratedge\\Inspection\\": "src/"
        }
    },
    "bin": [
        "bin/inspection"
    ],
    "require-dev": {
        "raveren/kint": "^1.0"
    },
    "minimum-stability": "stable"
}
